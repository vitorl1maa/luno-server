# Configurações do Servidor
PORT=8000

# JWT Secret - Token de autenticação
# IMPORTANTE: Use um valor seguro e aleatório em produção!
JWT_SECRET=seu_jwt_secret_aqui

# Configuração do Banco de Dados PostgreSQL (URL completa para Prisma)
DATABASE_URL=postgresql://postgres:admin@localhost:5432/luno_db

# Configuração do Banco de Dados PostgreSQL (variáveis individuais)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=admin
POSTGRES_DB=luno_db
